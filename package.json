{
  "name": "mcea-eleventy",
  "version": "1.0.0",
  "description": "Website for MCEA built using static website generator Eleventy",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/assets/sass:_site/css",
    "build:sass": "sass src/assets/sass:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.64.2"
  },
  "dependencies": {
    "sharp": "^0.32.4"
  }
}
